FLASK_ENV=development
FLASK_DEBUG=True
DATABASE_URL=sqlite:///j3d.db

# Etsy API Credentials - Get these from https://www.etsy.com/developers
ETSY_CLIENT_ID=your_etsy_client_id_here
ETSY_CLIENT_SECRET=your_etsy_client_secret_here
ETSY_REDIRECT_URI=http://localhost:4200/oauth-callback

# Session/JWT Secret - Change this to a random string in production
SECRET_KEY=your-super-secret-key-change-this-in-production

# ==================== PRINTER APIs (Optional) ====================
# Bambu Lab Cloud API
BAMBU_API_BASE_URL=https://api.bambulab.com

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:4200

# Backend URL (used by frontend)
BACKEND_URL=http://localhost:5000
